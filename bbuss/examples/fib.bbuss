#include "../lib/libc.bbuss"
#include "../lib/std.bbuss"
#define CLOCKS_PER_SEC 1000

func fib(int n) int {
    if(n le 1) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}


func main() int {

    ptr start = clock();

    int f1 = fib(43);

    ptr end = clock();

    int elapsed = (end - start)/CLOCKS_PER_SEC;


    printf("standard fib(43) = %d         ", f1);
    printf("        elapsed: %dms        ", elapsed);


    return 0;
}





