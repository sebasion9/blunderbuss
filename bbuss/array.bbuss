extern func printf(str fmt, any msg) any {}
extern func rand() int {}
extern func malloc(int size) ptr {}
extern func realloc(ptr p, int size) ptr {}

func fill(int size) int {
    ptr arr = malloc(size*8);
    for(int i = 0; i lt size; i = i + 1;) {
        arr[i] = i * 2;
    }
    return arr;
}

func main() int {

    int size = 100;
    ptr arr = fill(size);


    for(int i = 0; i lt size; i = i + 1;) {
        printf("%d  ", arr[i]);
    }

    return 0;
}
