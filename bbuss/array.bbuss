#include "lib/libc.bbuss"


func fill(int size) int {
    ptr arr = malloc(size*8);
    for(int i = 0; i lt size; i = i + 1;) {
        arr[i] = i * 2;
    }
    return arr;
}

func main() int {

    int size = 100;
    ptr arr = fill(size);


    for(int i = 0; i lt size; i = i + 1;) {
        printf("%d  ", arr[i]);
    }

    free(arr);

    return 0;
}
